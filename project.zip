<script>
    $(function() {
        var isJapan = (window.location.pathname.substr(0, 4) !== '/en/');
        var $email = $('#login-email');
        var $emailErrors = $('#login-email-errors');
        $email.on('keyup', _.debounce(function(e) {
            var errors = [];
            var data  = $email.val();

            if (!_.isString(data)) {
                return;
            }

            // todo: english
            if (/[　]/.test(data)) {
                errors.push('全角空白');
            }

            data = data.trim();

            if (!/^[a-zA-Z0-9.!#$%&'*+\/=?^_`{|}~\-@]*$/.test(data)) {
                if (/[＠]/.test(data)) {
                    errors.push('全角＠');
                }

                if (/[．]/.test(data)) {
                    errors.push('全角ドット')
                }

                if (errors.length === 0) {
                    errors.push('全角文字または不正な文字');
                }
            }

            if (errors.length === 0) {
                $emailErrors.hide();
            } else {
                $emailErrors.show();

                if (isJapan) {
                    $emailErrors.text('メールアドレスに' + errors.join(', ') + 'が含まれています');
                } else {
                    $emailErrors.text('Invalid email');
                }
            }
        }, 300));
    });
</script>
